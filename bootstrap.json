  "build": {
    "libs": [
      {
        "name": "jQuery",
        "javascript": ["//cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0/jquery.min.js"]
      },
      {
        "name": "Tether",
        "javascript": ["//cdnjs.cloudflare.com/ajax/libs/tether/1.3.2/js/tether.min.js"]
      },
      {
        "name": "Bootstrap 4",
        "javascript": ["//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0-alpha/js/bootstrap.min.js"],
        "stylesheet": ["//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0-alpha/css/bootstrap.css"]
